        <property name="hobbies">
            <set value-type="java.util.Set">
                <ref bean="HobbyRally"/>
                <ref bean="HobbyFishing"/>
                <ref bean="HobbyFootBall"/>
            </set>
        </property>
        <property name="places">
            <set value-type="java.util.Set">
                <ref bean="PlaceDnepr"/>
                <ref bean="PlaceKiev"/>
                <ref bean="PlaceOdessa"/>
            </set>
        </property>
    </bean>
    <bean id="Contact4" name="Contact4" class="CoreMyCpring.Contact">
        <property name="firstName" value="Vasilij"/>
        <property name="lastName" value="Ivanovich"/>
        <property name="hobbies">
            <set value-type="java.util.Set">
                <ref bean="HobbyRally"/>
                <ref bean="HobbyFishing"/>
