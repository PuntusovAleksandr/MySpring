            </set>
        </property>
        <property name="places">
            <set value-type="java.util.Set">
                <ref bean="PlaceDnepr"/>
                <ref bean="PlaceKiev"/>
            </set>
        </property>
    </bean>
    <bean id="Contact5" name="Contact5" class="CoreMyCpring.Contact">
        <property name="firstName" value="Oleg"/>
