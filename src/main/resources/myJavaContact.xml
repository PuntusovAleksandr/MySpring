<?xml version="1.0" encoding="UTF-8"?>
<!--suppress InjectionValueTypeInspection -->
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans 
                           http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

    <bean id="JavaContact" class="home.contact.ContactService.impl.JavaContactService">
        <property name="contactDao" ref="ContactDao"/>
        <property name="hobbyDao" ref="HobbyDao"/>
        <property name="messegeDao" ref="MessegeDao"/>
        <property name="placeDao" ref="PlaceDao"/>
    </bean>

    <bean id="ContactDao" class="home.contact.MyFilesDOT.inpl.ContactDao">
        <property name="contactlist">
            <list value-type="java.util.List">
                <ref bean="Contact1"/>
                <ref bean="Contact2"/>
                <ref bean="Contact3"/>
                <ref bean="Contact4"/>
                <ref bean="Contact5"/>
            </list>
        </property>
    </bean>
    <bean id="HobbyDao" class="home.contact.MyFilesDOT.inpl.HobbyDao">
        <property name="listHobby">
            <list value-type="java.util.ArrayList">
                <ref bean="HobbyRally"/>
                <ref bean="HobbyFishing"/>
                <ref bean="HobbyBoks"/>
                <ref bean="HobbyBoks"/>
            </list>
        </property>
    </bean>
    <bean id="MessegeDao" class="home.contact.MyFilesDOT.inpl.MessageDao"/>
    <bean id="PlaceDao" class="home.contact.MyFilesDOT.inpl.PlaceDao"/>

    <bean id="Contact1" name="Contact1" class="home.contact.CoreMyCpring.Contact">
        <property name="firstName" value="Aleks"/>
        <property name="lastName" value="Ivanovich"/>
        <property name="hobbies">
            <set value-type="java.util.Set">
                <ref bean="HobbyRally"/>
                <ref bean="HobbyFishing"/>
                <ref bean="HobbyFootBall"/>
                <ref bean="HobbyBoks"/>
            </set>
        </property>
        <property name="places">
            <set value-type="java.util.Set">
                <ref bean="PlaceDnepr"/>
                <ref bean="PlaceKiev"/>
                <ref bean="PlaceOdessa"/>
            </set>
        </property>
        <property name="birthDate" ref="localDate1"/>
    </bean>
    <bean id="Contact2" name="Contact2" class="home.contact.CoreMyCpring.Contact">
        <property name="firstName" value="Maks"/>
