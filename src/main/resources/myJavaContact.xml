            </set>
        </property>
        <property name="birthDate" ref="localDate3"/>
    </bean>

    <bean id="HobbyRally" name="Rally" class="home.contact.CoreMyCpring.Hobby">
        <property name="title" value="RallyOnTheCar"/>
        <property name="description" value="Like see on the car rally"/>
    </bean>
    <bean id="HobbyFishing" name="Fishing" class="home.contact.CoreMyCpring.Hobby">
        <property name="title" value="FishingBigFish"/>
        <property name="description" value="Like fishing big fish"/>
    </bean>
    <bean id="HobbyFootBall" name="FootBall" class="home.contact.CoreMyCpring.Hobby">
        <property name="title" value="FootBall"/>
        <property name="description" value="Like see and play football"/>
    </bean>
    <bean id="HobbyBoks" name="Boks" class="home.contact.CoreMyCpring.Hobby">
        <property name="title" value="Boks"/>
        <property name="description" value="Like see and play boks"/>
