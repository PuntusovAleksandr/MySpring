<?xml version="1.0" encoding="UTF-8"?>
<!--suppress InjectionValueTypeInspection -->
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans 
                           http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

    <bean id="JavaContact" class="ContactService.JavaContactService">
        <property name="contactDao" ref="ContactDao"/>
        <property name="hobbyDao" ref="HobbyDao"/>
        <property name="messegeDao" ref="MessegeDao"/>
        <property name="placeDao" ref="PlaceDao"/>
    </bean>

    <bean id="ContactDao" class="MyFilesDOT.ContactDao">
        <property name="contactlist">
            <list value-type="java.util.List">
                <ref bean="Contact1"/>
                <ref bean="Contact2"/>
                <ref bean="Contact3"/>
                <ref bean="Contact4"/>
                <ref bean="Contact5"/>
            </list>
        </property>
    </bean>
    <bean id="HobbyDao" class="MyFilesDOT.HobbyDao">
        <property name="listHobby">
            <list value-type="java.util.ArrayList">
                <ref bean="HobbyRally"/>
                <ref bean="HobbyFishing"/>
                <ref bean="HobbyBoks"/>
                <ref bean="HobbyBoks"/>
            </list>
        </property>
    </bean>
    <bean id="MessegeDao" class="MyFilesDOT.MessageDao"/>
    <bean id="PlaceDao" class="MyFilesDOT.PlaceDao"/>

    <bean id="Contact1" name="Contact1" class="CoreMyCpring.Contact">
        <property name="firstName" value="Aleks"/>
        <property name="lastName" value="Ivanovich"/>
        <property name="hobbies">
            <set value-type="java.util.Set">
                <ref bean="HobbyRally"/>
                <ref bean="HobbyFishing"/>
                <ref bean="HobbyFootBall"/>
                <ref bean="HobbyBoks"/>
            </set>
        </property>
        <property name="places">
            <set value-type="java.util.Set">
                <ref bean="PlaceDnepr"/>
                <ref bean="PlaceKiev"/>
                <ref bean="PlaceOdessa"/>
            </set>
        </property>
    </bean>
    <bean id="Contact2" name="Contact2" class="CoreMyCpring.Contact">
