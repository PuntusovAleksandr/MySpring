    <bean id="localDate1" class="java.time.LocalDate" factory-method="of">
    <constructor-arg index="0" value="1995" />
    <constructor-arg index="1" value="3" />
    <constructor-arg index="2" value="11" />
    </bean>
    <bean id="localDate2" class="java.time.LocalDate" factory-method="of">
        <constructor-arg index="0" value="2000" />
        <constructor-arg index="1" value="7" />
        <constructor-arg index="2" value="17" />
    </bean>
    <bean id="localDate3" class="java.time.LocalDate" factory-method="of">
        <constructor-arg index="0" value="1990" />
        <constructor-arg index="1" value="12" />
        <constructor-arg index="2" value="11" />
    </bean>

    <bean id="PlaceDnepr" name="placeDnepr" class="home.contact.CoreMyCpring.Place">
        <property name="title" value="Dnepr"/>
        <property name="description" value="My love city"/>
        <property name="latitude" value="49.0556126545216"/>
        <property name="longitude" value="84.5489415681"/>
    </bean>
    <bean id="PlaceKiev" name="placeKiev" class="home.contact.CoreMyCpring.Place">
        <property name="title" value="Kiev"/>
        <property name="description" value="My love big city"/>
        <property name="latitude" value="10.61651616"/>
        <property name="longitude" value="99.115651651"/>
    </bean>
    <bean id="PlaceOdessa" name="placeOdessa" class="home.contact.CoreMyCpring.Place">
        <property name="title" value="Odessa"/>
        <property name="description" value="My love sea city"/>
        <property name="latitude" value="0.5684984"/>
        <property name="longitude" value="0.26461265129"/>
    </bean>



</beans>